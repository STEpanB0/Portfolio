{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "exclude": ["node_modules"]
  }